@echo off
title SC Modding Tool Launcher Overengineered version
chcp 65001 >nul 2>&1
cls

:: Farben aktivieren (Windows 10+)
for /f "tokens=2 delims=[]" %%a in ('ver') do set "winver=%%a"
if %winver:~0,2% GEQ 10 color 0A

echo [SYSTEM]Starting the simple python script in a stupid long way...

:: Letzter Check auf Python
echo [SYSTEM] Checking for Python installation...
where python >nul 2>nul
if %errorlevel% neq 0 (
    echo [ERROR] Python not found!
    echo Please install Python from https://www.python.org/downloads/
    timeout /t 5
    exit /b
)
echo [STATUS] Python found! Proceeding...
timeout /t 1 >nul
echo.

:: OS-Check mit Fake-Erkennung
echo [SYSTEM CHECK] Scanning OS...
ping -n 2 127.0.0.1 >nul
for /f "tokens=2 delims==" %%O in ('wmic os get Caption /value ^| find "="') do set "OSNAME=%%O"
if "%OSNAME%"=="" set "OSNAME=Windows (Fake Check)"
::echo Detected OS: %OSNAME%
echo.
timeout /t 1 >nul

:: Fake TempleOS Check
echo [WARNING] TempleOS not found! 
timeout /t 1 >nul
echo [STATUS] Proceeding with alternative OS...
timeout /t 1 >nul
echo.

:: Episches ASCII-Logo
echo ========================================================
echo     SC Modding Tool - Overengineered Edition
echo ========================================================
echo.
timeout /t 2 >nul

:: Fake System-Optimierung
echo [INIT] Checking system compatibility...
timeout /t 1 >nul
echo [OPTIMIZATION] Enhancing CPU performance... 
ping -n 2 127.0.0.1 >nul
echo [MEMORY] Allocating additional 512MB of VRAM...
timeout /t 1 >nul
echo [PROTOCOL] Enabling modding API...
timeout /t 1 >nul
echo [LOADING] Preparing dependencies...
echo.
timeout /t 2 >nul


:: Fake-KI Initialisierung
echo [MOD SYSTEM] Initializing game file for optimization...
timeout /t 1 >nul
echo [MOD SYSTEM] Running starting script...
ping -n 2 127.0.0.1 >nul
echo [MOD SYSTEM] Predicting parameters...
timeout /t 1 >nul
echo [MOD SYSTEM] Process complete.
timeout /t 2 >nul
echo.


:: Epische Start-Animation
echo [START] Launching SC Modding Tool...
timeout /t 1 >nul
echo Initializing game data...
ping -n 2 127.0.0.1 >nul
echo Verifying modding parameters...
timeout /t 1 >nul
echo Checking modding permissions...
ping -n 2 127.0.0.1 >nul
echo Establishing secure connection to local modding server...
ping -n 2 127.0.0.1 >nul
echo Booting up modding UI...
timeout /t 1 >nul
echo Done!
echo Overengineered
timeout /t 2 >nul

:: Starte das eigentliche Skript
python sc.py
echo.
echo [EXIT] SC Modding Tool has been closed.
pause
